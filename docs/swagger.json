    "/water/day/{date}": {
      "get": {
        "tags": [
          "Water"
        ],
        "summary": "Get the volume of water per day",
        "operationId": "getWaterConsumptionByDate",
        "description": "Get the volume of water per day",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "in": "path",
            "name": "date",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            },
            "example": "2024-08-02"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully found information about water!",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "status",
                    "message",
                    "data"
                  ],
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "message": {
                      "type": "string",
                      "example": "Successfully found information about water!"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "required": [
                          "_id",
                          "amountOfWater",
                          "time",
                          "date",
                          "dailyNorma",
                          "totalAmount",
                          "userId"
                        ],
                        "properties": {
                          "_id": {
                            "type": "string",
                            "example": "66b3bde3e4eebc5433d97cd5"
                          },
                          "amountOfWater": {
                            "type": "number",
                            "example": 200
                          },
                          "time": {
                            "type": "string",
                            "example": "12:15"
                          },
                          "date": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2024-07-07T00:00:00.000Z"
                          },
                          "dailyNorma": {
                            "type": "number",
                            "example": 1.5
                          },
                          "totalAmount": {
                            "type": "number",
                            "example": 0
                          },
                          "userId": {
                            "type": "string",
                            "example": "66acbaff8654757d2fc9bdba"
                          },
                          "percentageConsumed": {
                            "type": "string",
                            "example": "13%"
                          },
                          "createdAt": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2024-07-07T00:00:00.000Z"
                          },
                          "updatedAt": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2024-07-07T00:00:00.000Z"
                          }
                        }
                      }
                    }
                  }
                },
                "examples": {
